#include "/home/rasse/.colors/erosionedit"
*scrollBar: false
!use powerline font as secondary
!*font: xft:Terminus:Regular:pixelsize=12:antialias=false:hinting=false:autohint=false, xft:Terminus (TTF) for Powerline:pixelsize=16:antialias=false:hinting=false:autohint=false
!*font: xft:Droid Sans Mono for Powerline:pixelsize=14:hinting=true:autohint=true, xft:PowerlineSymbols:pixelsize=14
*font: xft:Terminus:pixelsize=12:hinting=true:autohint=true
!, xft:PowerlineSymbols:pixelsize=14
!*font: xft:Menlo for Powerline:pixelsize=16:hinting=true:autohint=true
*urgentOnBell: true
*borderless: 1

!*perl-ext-common:  	default,url-select
!*keysym.M-u:			perl:url-select:select_next
!*url-select.launcher:	/usr/bin/firefox
!*url-select.underline: true

Xcursor.theme: Human
Xcursor.size: 48
!Xcursor.theme: whiteglass
!Xcursor.size: 12

!colors:
!A tango-esque light theme
!*foreground: #A0A0A0
urxvt*depth: 32

!*shading: 10
!*transparent: true
!*inheritPixmap: true
