source-file ~/.tmux.conf
set -g pane-border-fg colour239
set -g pane-border-bg default
set -g pane-active-border-fg colour239
set -g pane-active-border-bg default
set status off
respawn-pane -k '~/dev/node/nodifier/clients/cli/cli.js l'
splitw -v -p 70 -t 0
respawn-pane -k 'weechat -r "/layout apply servy;/bar hide status"'
#selectp -t 1
#clock-mode
